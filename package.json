  "scripts": {
    "test": "npm run build && nyc mocha",
    "lint": "tslint -p ./tsconfig.json -c ./tslint.json './src/**/*.ts'",
    "copy-schemas": "cpx \"src/schemas/*.graphql\" lib/schemas/",
    "build": "rm -rf lib && npm run lint && tsc && npm run copy-schemas",
    "cover": "npm run lint && nyc report --reporter=text-lcov | coveralls",
    "start": "npm run build && node -r ./init.js bin/starter.js"
  },
